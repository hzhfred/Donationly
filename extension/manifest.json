{
 "manifest_version": 2,
 "name": "ReaDonate",
 "version": "0.7.5",
 "permissions": ["activeTab", "GOOGLE CLOUD API ENDPOINT"],
 "description": "donate",
 "icons": {"16": "icon16.png",
           "48": "icon48.png",
           "128": "icon128.png" },
 "browser_action": {
  "default_icon": "icon128.png",
  "default_title": "donate",
  "default_popup": "index.html"
},
"content_scripts": [{
  "matches": ["<all_urls>"],
  "js": ["index.js"]
}]
}
